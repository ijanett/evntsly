<h3>You've RSVP'd to:</h3>

<section>
    <% @guest_events.each do |event| %>
    
        <h4> <%= event.name%> </h4>
        <p>
            <strong>Location:</strong> <%= event.location%><br>
            <strong>Date:</strong> <%= event.date%>
            <strong>| Time:</strong> <%= event.time%><br>
        </p>
    <%end%>
</section>